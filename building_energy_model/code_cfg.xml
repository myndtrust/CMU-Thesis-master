<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE BCVTB-variables SYSTEM "variables.dtd">

<BCVTB-variables>
 
<!-- Recieve from E+ -->
  <variable source="EnergyPlus">
   <EnergyPlus name="East Zone" type="Zone Thermostat Heating Setpoint Temperature"/>
  </variable>
  
  <variable source="EnergyPlus">
   <EnergyPlus name="East Zone" type="Zone Thermostat Cooling Setpoint Temperature"/>
  </variable> 

  <variable source="EnergyPlus">
   <EnergyPlus name="Whole Building" type="Facility Total Building Electric Demand Power"/>
  </variable> 

  <variable source="EnergyPlus">
   <EnergyPlus name="Whole Building" type="Facility Total Electric Demand Power"/>
  </variable> 

  <variable source="EnergyPlus">
   <EnergyPlus name="Whole Building" type="Facility Total HVAC Electric Demand Power"/>
  </variable> 


<!-- Send to E+ -->
 <variable source="Ptolemy">
   <EnergyPlus schedule="HeatingSetPoint_RL"/>
 </variable>
 
 <variable source="Ptolemy">
   <EnergyPlus schedule="CoolingSetPoint_RL"/>
 </variable>

 <variable source="Ptolemy">
   <EnergyPlus schedule="Data Center CPU Loading Schedule_RL"/>
 </variable>
 
</BCVTB-variables>